node{

   stage('SCM Checkout'){
     git 'https://github.com/desai246/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
      def mvnHome =  tool name: 'mavenlatest', type: 'maven'   
      sh "${mvnHome}/bin/mvn package"
   } 
   stage('Deploy to Tomcat'){
        sshagent(['tomcat-dev']) {
           sh 'scp -o StrictHostKeyChecking=no target/*.war ec2-user@http://34.218.58.4:/opt/apache-tomcat-8.5.35/webapp'
         }

    }
